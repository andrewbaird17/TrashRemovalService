@{
    ViewData["Title"] = "SeeOnMap";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Location on Map</h1>

<div class="row">
    <div class="col-md-6">

    </div>
    <div class="col-md-6">
        <div id="map" style="height:500px; width:500px"></div>
        <script>
            var map;
            function initMap() {
                map = new google.maps.Map(document.getElementById('map'), {
                    center: { lat: 43.041, lng: -87.906 },
                    zoom: 12
                });
            }
        </script>
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAFLQhvZ2wS9E17RBvrs8SgyPHKs2E46Fk&callback=initMap"
                async defer></script>
    </div>
</div>